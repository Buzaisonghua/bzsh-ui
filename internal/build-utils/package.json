    "@pnpm/find-workspace-packages": "^4.0.16",
    "@pnpm/logger": "^4.0.0",
    "consola": "^2.15.3"
  },
  "devDependencies": {
    "@pnpm/types": "^1000.1.0",
    "unbuild": "^3.3.1"
  }
}
