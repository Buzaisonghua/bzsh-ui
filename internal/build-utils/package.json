{
  "name": "@bzsh-ui/build-utils",
  "version": "0.0.1",
  "private": true,
  "description": "Build utils for Bzsh Ui",
  "keywords": [
    "bzsh-ui"
  ],
  "license": "MIT",
  "main": "./dist/index.cjs",
